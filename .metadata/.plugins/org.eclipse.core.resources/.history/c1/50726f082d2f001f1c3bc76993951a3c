package entities;

public class Employee {

	// ATRIBUTOS
	private String name;
	private Integer hours;
	private Double valuePerHours;
	
	// CONSTRUTOR
	public Employee() {
		
	}

	public Employee(String name, Integer hours, Double valuePerHours) {
		this.name = name;
		this.hours = hours;
		this.valuePerHours = valuePerHours;
	}
	
	
}